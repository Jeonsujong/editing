<p id="notice"><%= notice %></p>

<h1>Manuals</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Skintype</th>
      <th>Age</th>
      <th>Atopy</th>
      <th>Pimple</th>
      <th>Allergy</th>
      <th>Bb</th>
      <th>Lip</th>
      <th>Eyebrow</th>
      <th>Eyeline</th>
      <th>Color</th>
      <th>Skincolor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manuals.each do |manual| %>
      <tr>
        <td><%= manual.title %></td>
        <td><%= manual.content %></td>
        <td><%= manual.skintype %></td>
        <td><%= manual.age %></td>
        <td><%= manual.atopy %></td>
        <td><%= manual.pimple %></td>
        <td><%= manual.allergy %></td>
        <td><%= manual.bb %></td>
        <td><%= manual.lip %></td>
        <td><%= manual.eyebrow %></td>
        <td><%= manual.eyeline %></td>
        <td><%= manual.color %></td>
        <td><%= manual.skincolor %></td>
        <td><%= link_to 'Show', manual %></td>
        <td><%= link_to 'Edit', edit_manual_path(manual) %></td>
        <td><%= link_to 'Destroy', manual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manual', new_manual_path %>
